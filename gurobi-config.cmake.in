cmake_minimum_required(VERSION 3.1)

@PACKAGE_INIT@

include(CMakeFindDependencyMacro)

set(GUROBI_VERSION_MAJOR "@PROJECT_VERSION_MAJOR@")
set(GUROBI_VERSION_MINOR "@PROJECT_VERSION_MINOR@")
set(GUROBI_VERSION_PATCH "@PROJECT_VERSION_PATCH@")
set(GUROBI_VERSION "@PROJECT_VERSION@")

find_dependency(Threads)

set_and_check(GUROBI_INCLUDE_DIRS "@PACKAGE_GUROBI_INCLUDE_DIR@")
set_and_check(GUROBI_LIBRARY_DIRS "@PACKAGE_GUROBI_LIBRARY_DIR@")
set(GUROBI_LIBRARIES gurobi "@LIBM_LIBRARY@" Threads::Threads)
@SET_GUROBI_CPP_LIBRARIES@

include("${CMAKE_CURRENT_LIST_DIR}/gurobi-targets.cmake")
@INCLUDE_GUROBI_CPP_TARGETS@
@INCLUDE_GUROBI_JAVA_TARGETS@
